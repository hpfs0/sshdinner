<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.dinner.gts.model">

     <class name="Notice" table="TT_NOTICE">
        <id name="noticeId" column="NOTICE_ID">
            <generator class="assigned" />
        </id>
        <property name="noticeHead" column="NOTICE_HEAD"/>
        <property name="noticeContent" column="NOTICE_CONTENT"/>
        <property name="RegistTime" column="REGIST_TIME"/>
        <property name="updateTime" column="UPDATE_TIME"/>
        <property name="deleteFlg" column="DELETE_FLG"/>
    </class>

</hibernate-mapping>